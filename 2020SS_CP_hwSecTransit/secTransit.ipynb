    "a = input('秒數: ')\n",
    "a = int(a)\n",
    "if(a<=60):\n",
    "    print(str(a) + '秒')\n",
    "elif(a<=3600):\n",
    "    b = a//60\n",
    "    c = a%60\n",
    "    print(str(b) + '分' + str(c) + '秒')\n",
    "else:\n",
    "    b = a//60\n",
    "    c = a%60\n",
    "    d = b//60\n",
    "    b = b%60\n",
    "    print(str(d) + '小時' + str(b) + '分' + str(c) + '秒')"
